<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>AHC_UI_errorRunningHealthCheck</name>
        <uuid>e9204179-1e44-4efc-bf10-8c2fae9d54e8</uuid>
        <description>Generic interface to display the Health Check error message</description>
        <parentUuid>_a-0000dbd4-7e1b-8000-4afc-010000010000_2068</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>=#&quot;SYSTEM_SYSRULES_formLayout&quot;(
  label: &quot;Appian Health Check Error&quot;,
  instructions: &quot;An error occurred while running the Appian Health Check. Please review the error message below and restart the request.&quot;,
  firstColumnContents: {
    #&quot;SYSTEM_SYSRULES_paragraphField&quot;(
      value: ri!errorMessage,
      readOnly: true
    )
  },
  buttons: #&quot;SYSTEM_SYSRULES_ButtonLayout&quot;(
    primaryButtons: {
      #&quot;SYSTEM_SYSRULES_ButtonWidgetSubmit&quot;(
        label: &quot;Close&quot;,
        style: &quot;PRIMARY&quot;,
        saveInto: {}
      )
    }
  ),
  validations: {}
)</definition>
        <namedTypedValue>
            <name>errorMessage</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>interface</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
        <role allowForAll="false" inherit="true" name="readers">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="true" name="authors">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="true" name="administrators">
            <users>
                <userUuid>Administrator</userUuid>
            </users>
            <groups/>
        </role>
        <role allowForAll="false" inherit="false" name="denyReaders">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="false" name="denyAuthors">
            <users/>
            <groups/>
        </role>
        <role allowForAll="false" inherit="false" name="denyAdministrators">
            <users/>
            <groups/>
        </role>
    </roleMap>
</contentHaul>
