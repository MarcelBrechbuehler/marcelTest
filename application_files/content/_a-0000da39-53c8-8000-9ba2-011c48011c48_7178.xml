    <rule>
        <name>SOR_integrationCheckSORN</name>
        <uuid>_a-0000da39-53c8-8000-9ba2-011c48011c48_7178</uuid>
        <description>check if party class is &quot;Individual&quot; and FirstName and Surname are not provided then SORN must be provided also if party class is &quot;Organisation&quot; and Name is not provided the SORN must be provided
</description>
        <parentUuid>_a-0000da88-6c19-8000-9ba2-011c48011c48_11453</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= if(
  and(
    ri!partyClass_txt = #&quot;_a-0000da0f-2564-8000-9ba2-011c48011c48_5217&quot;,
    len(
      ri!firstName_txt
    )=0,
    len(
      ri!surname_txt
    )=0
  ),
  if(
    len(
      ri!SORN_txt
    )=0,
    #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_7111&quot;(
      #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_7108&quot;(
        823,
        838,
        &quot;7&quot;,
        819
      ),
      ri!field_txt,
      ri!SORN_txt
    ),
    {}
  ),
  if(
    and(
      ri!partyClass_txt = #&quot;_a-0000da0f-2564-8000-9ba2-011c48011c48_5218&quot;,
      len(
        ri!name_txt
      )=0,
      len(
        ri!businessNumber_int
      )=0
    ),
    if(
      isnull(
        ri!SORN_txt
      ),
      #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_7111&quot;(
        #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_7108&quot;(
          823,
          838,
          &quot;9&quot;,
          819
        ),
        ri!field_txt,
        ri!SORN_txt
      ),
      {}
    ),
    {}
  )
)</definition>
