      #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_8409&quot;
    ),
    ri!field_txt,
    ri!mineralType
  ),
  if(
    contains(
      #&quot;_a-0000da72-0b7d-8000-9ba2-011c48011c48_10279&quot;(
        #&quot;_a-0000da0f-2564-8000-9ba2-011c48011c48_4414&quot;,
        #&quot;_a-0000da72-0b7d-8000-9ba2-011c48011c48_10281&quot;
