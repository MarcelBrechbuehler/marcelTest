        <uuid>_a-0000da39-53c8-8000-9ba2-011c48011c48_8616</uuid>
        <description>This expression rule is to check if mineral type is provided and is valid</description>
        <parentUuid>_a-0000da88-6c19-8000-9ba2-011c48011c48_11453</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
