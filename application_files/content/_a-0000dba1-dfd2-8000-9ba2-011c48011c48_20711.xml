<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>CaRE_updateCaREQuestionAnswerStatus</name>
        <uuid>_a-0000dba1-dfd2-8000-9ba2-011c48011c48_20711</uuid>
        <description>Update a CaRE QuestionAnswer status for the given questionAnswerId (CTCaREEvaluationLink)</description>
        <parentUuid>_a-0000d9e6-bfac-8000-9ba2-011c48011c48_1188</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>if(#&quot;_a-0000db6c-d5e0-8000-9ba2-011c48011c48_12573&quot;(ri!questionAnswerId),
  {},
  with(
    local!questionAnswer: #&quot;_a-0000d9e6-bfac-8000-9ba2-011c48011c48_1558&quot;(ri!questionAnswerId),
    local!updatedQuestionAnswer:  if(#&quot;_a-0000db6c-d5e0-8000-9ba2-011c48011c48_12573&quot;(local!questionAnswer),
                                    {},
                                    'type!{http://www.ehp.qld.gov.au/connect/types/}CTCaREQuestionAnswer'(
                                      careQuestionAnswerId: local!questionAnswer.careQuestionAnswerId,
                                      careEvaluationComponentId:  local!questionAnswer.careEvaluationComponentId,
                                      careQuestionId: local!questionAnswer.careQuestionId,
                                      source: local!questionAnswer.source,
                                      answerSource: local!questionAnswer.answerSource,
                                      status: ri!statusId,
                                      createdBy:  local!questionAnswer.createdBy,
                                      createdTimestamp: local!questionAnswer.createdTimestamp,
                                      updatedBy:  loggedInUser(),
                                      updatedTimestamp: now(),
                                      active: local!questionAnswer.active
                                    )
                                  ),
    local!updatedQuestionAnswer                               
  )
)</definition>
        <namedTypedValue>
            <name>questionAnswerId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>statusId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
