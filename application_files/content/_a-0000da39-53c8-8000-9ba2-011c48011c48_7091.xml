    <rule>
        <name>SOR_integrationCheckPartyClass</name>
        <uuid>_a-0000da39-53c8-8000-9ba2-011c48011c48_7091</uuid>
        <description>check if party class is either &quot;Individual&quot; or &quot;Organisation&quot; otherwise return false</description>
        <parentUuid>_a-0000da88-6c19-8000-9ba2-011c48011c48_11453</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>= if(
  len(
    ri!partyClass_txt
  )=0,
  #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_7111&quot;(
    #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_7108&quot;(
      823,
      838,
      &quot;5&quot;,
      819
    ),
    ri!field_txt,
    ri!partyClass_txt
  ),
  if(
    or(
      ri!partyClass_txt = #&quot;_a-0000da84-7836-8000-9ba2-011c48011c48_11285&quot;,
      lookup(#&quot;_a-0000da88-6c19-8000-9ba2-011c48011c48_13365&quot;,ri!partyClass_txt,-1)&gt;0
    ),
    {},
    #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_7111&quot;(
      #&quot;_a-0000da39-53c8-8000-9ba2-011c48011c48_7108&quot;(
        823,
        838,
        &quot;2&quot;,
        819
      ),
      ri!field_txt,
      ri!partyClass_txt
    )
  )
)</definition>
        <namedTypedValue>
            <name>partyClass_txt</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>field_txt</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
